stack = []

def push(n):
  if len(stack) == n:
    print("The stack is full")
  else:
    element = input("Enter the element you want to push: ")
    if element.isdigit():
      stack.append(int(element))
    else:
      stack.append(element)
  print(stack)

def pop():
  if not stack:
    print("The stack is empty")
  else:
    e = stack.pop()
    print(f"The removed element is {e}")
    print(stack)

stack_size = int(input("Enter the size of stack: "))
while True:
  option_input = input("Enter option: 1. Push into stack 2. Pop from stack 3. Quit ")

  if option_input == "1":
    push(stack_size)
  elif option_input == "2":
    pop()
  elif option_input == "3":
    break
  else:
    print("Print correct input")
    
